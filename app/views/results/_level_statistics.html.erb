<div id="<%= level %>-team">
  <table>
    <colgroup>
      <col width="40"/>
    </colgroup>
    <tr>
      <td colspan="5" class="separator">
				<%= params[:klass].capitalize %> School 
				<%= level.capitalize %> Team Results
			</td>
    </tr>
    <tr>
      <th>Rank</th>
      <th>School Name</th>
      <th>Student Score Sum</th>
      <th>Test Score</th>
      <th>Total Score</th>
    </tr>
    <% teams.each do |team| %>
      <tr class="<%= cycle('even', 'odd') %>">
        <td class="num"><%= rank(team.team_score, "team_#{level}") %></td>
        <td><%= team_name(team) %></td>

        <td class="num">
					<%= team.student_score_sum %> 
					<%= check_image(team, :student_scores_checked) %>
				</td>
        <td class="num">
					<%= team.team_test_score %> 
					<%= check_image(team, :team_score_checked) %>
				</td>
        <td class="num">
					<%= team.team_score %> 
					<%= check_image(team) %>
				</td>
      </tr>
    <% end %>
  </table>
</div>

<div id="<%= level %>-individual">
  <table>
    <colgroup>
      <col width="40"/>
    </colgroup>
    <tr>
      <td colspan="4" class="separator">
				<%= params[:klass].capitalize %> School Individual 
				<%= level.capitalize %> Results
			</td>
    </tr>
    <tr>
      <th>Rank</th>
      <th>Name</th>
      <th>School</th>
      <th>Score</th>
    </tr>
    <% students.each do |student| %>
      <tr class="<%= cycle('even', 'odd') %>">
        <td class="num"><%= rank(student.test_score, "student_#{level}") %></td>
        <td><%= student.name %></td>
        <td><%= team_name(student.team) %></td>
        <td class="num"><%= student.test_score %> <%= check_image(student) %></td>
      </tr>
    <% end %>
  </table>
</div>
