<head>
  <meta http-equiv="Content-type" content="text/html; charset=utf-8"/>
  <title>Golden Circle</title>
  <%= stylesheet_link_tag "blueprint/screen.css", :media => "screen, projection, print" %>
  <%= stylesheet_link_tag "blueprint/print.css", :media => "print" %>
  <!--[if IE]><%= stylesheet_link_tag "blueprint/ie.css", :media => "screen, projection" %><![endif]-->
  <%= stylesheet_link_tag "style.css", :media => "screen, print" %>
  <%= stylesheet_link_tag "print.css", :media => "print" %>
  <%= javascript_include_tag 'jquery', 'application', 'corner' %>
  <%= yield(:jsinclude) %>
  <% javascript_tag do %>
    <%= yield(:jstemplates) %>
    <%= yield(:js) %>
    $(function(){
      <%= yield(:jsload) %>
    });
  <% end %>
</head>
